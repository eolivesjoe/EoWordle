<Window x:Class="EoWordle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:EoWordle"
        xmlns:vm="clr-namespace:EoWordle.ViewModels"
        Title="EoWordle" Height="450" Width="800">
    <Window.DataContext>
        <vm:GameViewModel />
    </Window.DataContext>
    <Grid>
        <ContentControl Content="{Binding CurrentView}" />
    </Grid>
</Window>